{
  "context": {
    "fileName": [
      "AGENTS.md",
      "GEMINI.md"
    ]
  },
  "general": {
    "checkpointing": {
      "enabled": true
    },
    "preferredEditor": "code",
    "vimMode": false
  },
  "ide": {
    "enabled": true,
    "hasSeenNudge": true
  },
  "mcpServers": {
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PAT_GEMINI}"
      }
    }
  },
  "ui": {
    "hideBanner": true,
    "hideTips": true,
    "showCitations": false,
    "showLineNumbers": true,
    "showMemoryUsage": true,
    "theme": "Google Code"
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  }
}