export NB_DEFAULT_EXTENSION="${NB_DEFAULT_EXTENSION:-md}"
export NB_DIR="${NB_DIR:-$HOME/notes}"
export NB_ENCRYPTION_TOOL="${NB_ENCRYPTION_TOOL:-gpg}"
export NB_FOOTER="${NB_FOOTER:-1}"
export NB_HEADER="${NB_HEADER:-3}"
export NB_LIMIT="${NB_LIMIT:-15}"
