#!/usr/bin/env bash

export EDITOR="vim"
export NB_AUTO_SYNC="${NB_AUTO_SYNC:-1}"
export NB_COLOR_PRIMARY="${NB_COLOR_PRIMARY:-69}"
export NB_COLOR_SECONDARY="${NB_COLOR_SECONDARY:-8}"
