# BINDKEY
bindkey -v
# PLUGINS
plugins=(
    vi-mode
    alias-tips
    zsh-autopair
    colored-man-pages
    zsh-autosuggestions
    zsh-syntax-highlighting
)
# SOURCES
source $ZSH/oh-my-zsh.sh
# ALIASES
alias -g A='| ag'
alias -g C='| unpigz -c'
alias -g D='| head'
alias -g F='| fzf'
alias -g G='| grep'
alias -g H='--help'
alias -g J='| jq -r'
alias -g K='| xsel --clipboard'
alias -g L='| less'
alias -g N='| notify-send'
alias -g P='| tqdm'
alias -g S='| sort'
alias -g T='| tail'
alias -g U='| uniq'
alias -g V='| vim -'
alias -g W='| wc -l'
alias -g Z='| pigz'
source ~/.aliases
