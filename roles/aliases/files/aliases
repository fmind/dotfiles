# a:ansible-playbook
alias a="ansible-playbook"
# b:bat (cat)
for cmd in bat batcat; do
    if command -v $cmd >/dev/null 2>&1; then
        alias b=$cmd
        break
    fi
done
# c:gcloud
alias c="gcloud"
# d:docker
alias d="docker"
# e:silver (grep)
alias e="ag"
# f:fd (find)
for cmd in fd fdfind; do
    if command -v $cmd >/dev/null 2>&1; then
        alias f=$cmd
        break
    fi
done
# g:git
alias g="git"
# h:http (curl)
alias h="http"
# i:gemini
alias i="gemini"
# j:just
alias j="just"
# k:kube
alias k="kubectl"
# l:ls
alias l="ls"
# m:make
alias m="make"
# n:npm
alias n="npm"
# o:htop
alias o="htop"
# p:python
alias p="python"
# q:copilot
alias q="gh copilot suggest"
# r:ptrepl
alias r="ptrepl"
# s:zsh
alias s="zsh"
# t:terraform
alias t="terraform"
# u:uv
alias u="uv"
# v:vscode (vim)
alias v="code"
# w:watchmedo
alias w="watchmedo"
# x:fzf
alias x="fzf"
# y:ptpython
