# a:openai
alias a="openai api chat.completions.create -m gpt-3.5-turbo --stream -g user"
# b:ansible-playbook
alias b="ansible-playbook"
# c:bat (cat)
for cmd in bat batcat; do
    if command -v $cmd >/dev/null 2>&1; then
        alias b=$cmd
        break
    fi
done
# d:docker
alias d="docker"
# e:silver (grep)
alias e="ag"
# f:fd (find)
for cmd in fd fdfind; do
    if command -v $cmd >/dev/null 2>&1; then
        alias f=$cmd
        break
    fi
done
# g:git
alias g="git"
# h:http (curl)
alias h="http"
# i:invoke (make)
alias i="inv"
# j:jq
alias j="jq"
# k:kube
alias k="kubectl"
# l:ls
alias l="ls"
# m:make
alias m="make"
# n:ncdu
alias n="ncdu"
# o:poetry
alias o="poetry"
# p:python
alias p="python"
# q:quit
alias q="exit"
# r:ptrepl
alias r="ptrepl"
# s:copilot
alias s="gh copilot suggest --target shell"
# t:htop
alias t="htop"
# u:fuck
alias u="fuck"
# v:vscode (vim)
alias v="code"
# w:watchmedo
alias w="watchmedo"
